# A sample Gemfile
source 'https://rubygems.org'

gem 'rails'
gem 'mysql2'
gem 'devise'
gem 'omniauth'
gem 'omniauth-github'
gem 'inherited_resources'

gem 'awesome_print'

group :development do
  gem 'capistrano'
  gem 'capistrano_colors'
end

group :production do
  gem 'unicorn'
end

group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
gem 'haml-rails'
